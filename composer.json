    "type": "project",
    "require": {
        "php": "^8.1",
        "brick/math": "^0.11",
        "doctrine/dbal": "^3.7",
        "guzzlehttp/guzzle": "^7.8",
        "league/csv": "^9.11",
        "monolog/monolog": "^3.5",
        "ramsey/uuid": "^4.9",
        "symfony/cache": "^6.4",
        "symfony/console": "^6.4",
        "vlucas/phpdotenv": "^5.6"
